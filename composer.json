        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "noam148/yii2-image-manager": "*",
        "overtrue/wechat": "~4.0",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "kartik-v/dependent-dropdown": "@dev",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "overtrue/easy-sms": "dev-master",
        "mdmsoft/yii2-admin": "2.x-dev",
        "paulzi/yii2-adjacency-list": "dev-master",
        "trntv/yii2-file-kit": "dev-master",
        "mohorev/yii2-upload-behavior": "*",
